{
  "name": "@libs/ui-kit",
  "repository": "https://github.com/let-us-group-up/libs.git",
  "version": "0.0.0",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./components/*": "./dist/components/*",
    "./icons/*": "./dist/icons/*",
    "./styles": "./dist/styles/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [ "dist/*", "dist/*/index.d.ts" ]
    }
  },
  "publishConfig": {
    "@libs/registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build:ts": "tsc --project tsconfig.prod.json",
    "build": "yarn build:ts",
    "watch": "yarn build:ts -w --preserveWatchOutput",
    "test": "exit 0",
    "clean": "rm -rf ./dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0-alpha-c76e4dbbc-20210722",
    "react-dom": ">=18.0.0-alpha-c76e4dbbc-20210722"
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-beta.1",
    "@material-ui/icons": "^5.0.0-beta.1",
    "color": "^4.0.0"
  }
}
